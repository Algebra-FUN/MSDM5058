   "source": [
    "using SymPy"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "(M, V, γ, γᵤ, ξ)"
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "@syms M V γ γᵤ ξ"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\frac{V \\left(γᵤ - 1\\right)}{M γᵤ ξ - V}$"
      ],
      "text/plain": [
       "V⋅(γᵤ - 1)\n",
       "──────────\n",
       "M⋅γᵤ⋅ξ - V"
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "A_U = ((1-γᵤ)*V)/((1-γᵤ)*M+V-M+(1-ξ)*γᵤ*M) |> simplify\n",
    "A_U"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$\\left[ \\begin{array}{r}\\frac{V γ}{M γ ξ - M ξ + V}\\end{array} \\right]$\n"
      ],
      "text/plain": [
       "1-element Vector{Sym}:\n",
       " V*γ/(M*γ*ξ - M*ξ + V)"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "solve(Eq(1-γ,A_U),γᵤ)"
   ]
  }
